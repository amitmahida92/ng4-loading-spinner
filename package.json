    "run-sequence": "^1.2.2",
    "rxjs": "^6.1.0",
    "rxjs-system-bundle": "^6.0.0",
    "systemjs": "^0.20.12",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": ">=2.7.2 <2.8",
    "zone.js": "^0.8.26"
  },
  "engines": {
    "node": ">=6.0.0"
  }
